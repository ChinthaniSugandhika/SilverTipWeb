<script>
    jQuery(document).ready(function () {
        jQuery(".select2").select2({
            width: '100%'
        });

        // Date Picker
        jQuery('#datepicker').datepicker();
        jQuery('#datepicker-inline').datepicker();
        jQuery('#datepicker-multiple').datepicker({
            numberOfMonths: 3,
            showButtonPanel: true
        });


    });

    function showTable() {
        $("#resultTable").show();
    }

    $('.openMod').click(function () {
        $(".child").show();
        resize();

    });

    $('.open-left').click(function () {
        resize();
    });

    function resize() {
        var divWidth = $(".content").innerWidth();
        var divHeight = $(".content").innerHeight();
        divHeight = divHeight + 55;
        $(".child").height(divHeight);
        $(".child").width(divWidth);
    }

    $('.child .closer').click(function () {
        $(".child").hide();
        $('.sup-overlay').hide();
        $('.personal-class').removeClass("bg-green");
        $('.financial-class').removeClass("bg-green");
        $('.facilities-class').removeClass("bg-green");
        $('.tea-class').removeClass("bg-green");
        removefacilityadd('all');
    });


    $(".content").resize(function () {
        var $this = $(this);
        var parentwidth = $this.innerWidth();
        var parentheight = $this.innerHeight();
        $(".child").css({
            'width': parentwidth,
            'height': parentheight
        });
    });

    function supload(whattoload) {
        //$('.sup-main-details .supplier-link').hide();
        if (whattoload == 'personal') {
            $('.personal').show();
            $('.financial').hide();
            $('.facilities').hide();
            $('.tea').hide();
            $('.tea-details').hide();

            $('.personal-class').addClass("bg-green");
            $('.financial-class').removeClass("bg-green");
            $('.facilities-class').removeClass("bg-green");
            $('.tea-class').removeClass("bg-green");

        } else if (whattoload == 'financial') {
            $('.personal').hide();
            $('.financial').show();
            $('.facilities').hide();
            $('.tea').hide();
            $('.tea-details').hide();

            $('.personal-class').removeClass("bg-green");
            $('.financial-class').addClass("bg-green");
            $('.facilities-class').removeClass("bg-green");
            $('.tea-class').removeClass("bg-green")
        } else if (whattoload == 'facilities') {
            $('.personal').hide();
            $('.financial').hide();
            $('.facilities').show();
            $('.tea').hide();
            $('.tea-details').hide();

            $('.personal-class').removeClass("bg-green");
            $('.financial-class').removeClass("bg-green");
            $('.facilities-class').addClass("bg-green");
            $('.tea-class').removeClass("bg-green");
        } else if (whattoload == 'tea') {
            $('.personal').hide();
            $('.financial').hide();
            $('.facilities').hide();
            $('.tea').show();
            $('.tea-details').hide();

            $('.personal-class').removeClass("bg-green");
            $('.financial-class').removeClass("bg-green");
            $('.facilities-class').removeClass("bg-green");
            $('.tea-class').addClass("bg-green");
        } else if (whattoload == 'tea-details') {
            $('.personal').hide();
            $('.financial').hide();
            $('.facilities').hide();
            $('.tea').hide();
            $('.tea-details').show();

        }


    }

    /*$('.supplier-link').click(function () {

        $('.sup-main-details .supplier-link').hide();
        $('.sup-overlay').show();
    });*/



    $('.tea-details-close').click(function () {
        $('.tea-details').hide();
        $('.tea').show();
        $('.tea-class').addClass("bg-green");
    });

    $('.sup-more-details-close').click(function () {
        $('.sup-main-details .supplier-link').show();
        $('.sup-overlay').hide();
        $('.personal-class').removeClass("bg-green");
        $('.financial-class').removeClass("bg-green");
        $('.facilities-class').removeClass("bg-green");
        $('.tea-class').removeClass("bg-green");
        removefacilityadd('all');
    });


    function addfacilities(facilitytype) {
        $('#facilities').removeClass("col-md-12");
        $('#facilities').addClass("col-md-6");

        $('#facilities-inner').removeClass("col-md-6");
        $('#facilities-inner').addClass("col-md-12");


        if (facilitytype == 'cash') {

            $('#facilities-cashadd').show();
            $('#facilities-fertiadd').hide();
            $('#facilities-teaadd').hide();
            $('#facilities-loadadd').hide();

        } else if (facilitytype == 'debt') {
            $('#debt').show();
        } else if (facilitytype == 'transport') {
            $('#trans').show();
        } else if (facilitytype == 'fertilizer') {

            $('#facilities-fertiadd').show();
            $('#facilities-cashadd').hide();
            $('#facilities-teaadd').hide();
            $('#facilities-loadadd').hide();

        } else if (facilitytype == 'fund') {
            $('#fundadd').show();
        } else if (facilitytype == 'tea') {

            $('#facilities-teaadd').show();
            $('#facilities-cashadd').hide();
            $('#facilities-fertiadd').hide();
            $('#facilities-loadadd').hide();

        } else if (facilitytype == 'loan') {

            $('#facilities-loadadd').show();
            $('#facilities-cashadd').hide();
            $('#facilities-fertiadd').hide();
            $('#facilities-teaadd').hide();

        }

    }

    function removefacilityadd(fertiremtype) {
        if (fertiremtype == 'cash') {
            $('#facilities-cashadd').hide();
        }
        else if (fertiremtype == 'loan') {
            $('#facilities-loadadd').hide();
        }
        else if (fertiremtype == 'ferti') {
            $('#facilities-fertiadd').hide();
        }
        else if (fertiremtype == 'tea') {
            $('#facilities-teaadd').hide();
        }
        else if (fertiremtype == 'all') {
            $('#facilities-cashadd').hide();
            $('#facilities-loadadd').hide();
            $('#facilities-fertiadd').hide();
            $('#facilities-teaadd').hide();
        }

        $('#facilities').removeClass("col-md-6");
        $('#facilities').addClass("col-md-12");

        $('#facilities-inner').removeClass("col-md-12");
        $('#facilities-inner').addClass("col-md-6");


    }

    function updatefacilityadd(fertiuptype) {

    }

    function addfunddetails() {
        $('.add-fund-details').show();
    }

    function showNewReg() {
        $("#resultTable").hide();
        $('#newsupplier').show();
    }
    function hideNewReg() {
        $('#newsupplier').hide();
    }



    $('#inlineRadio2').click(function () {
        $('.givingofficer').show();
        $('.thebr').hide();

    });

    $('#inlineRadio1').click(function () {
        $('.givingofficer').hide();
        $('.thebr').show();
    });

    $('#offiissuet').click(function () {
        $('.givingofficer').show();
        $('.thebr').hide();

    });

    $('#newissuet').click(function () {
        $('.givingofficer').hide();
        $('.thebr').show();
    });

</script>